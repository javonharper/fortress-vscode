{
  "name": "Fortress",
  "type": "light",
  "colors": {
    "editor.background": "#f5f5f5",
    "editor.foreground": "#333333",
    "list.activeSelectionIconForeground": "#fff"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment"],
      "settings": {
        "foreground": "#AAAAAA"
      }
    },
    {
      "name": "Raw values",
      "scope": ["string.quoted", "string.template", "constant"],
      "settings": {
        "foreground": "#0090c9"
      }
    },
    {
      "name": "Variable keyword",
      "scope": ["storage"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type.primitive"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tag",
      "scope": ["support.class.component"],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tag Attribute name",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#002c8a"
      }
    },
    {
      "name": "Braces for JSX Variables",
      "scope": ["punctuation.section.embedded"],
      "settings": {
        "foreground": "#0090c9"
      }
    },

    {
      "name": "Test",
      "scope": ["keyword.control"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Test",
      "scope": ["keyword.operator"],
      "settings": {
        "fontStyle": "bold"
      }
    }
    // {
    //   "name": "Test",
    //   "scope": ["keyword"],
    //   "settings": {
    //     "foreground": "#FF00FF"
    //   }
    // }
  ]
}
